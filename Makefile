Library		= libft

files	= ft_strlen \
			 ft_isalpha \
			 ft_isdigit \
			 ft_isalnum \
			 ft_isascii \
			 ft_isprint \
			 ft_toupper \
			 ft_tolower \
			 ft_strncmp \
			 ft_atoi \
			 ft_strdup \
			 ft_strjoin \
			 ft_putchar_fd \
			 ft_putstr_fd \
			 ft_putendl_fd \
			 ft_putnbr_fd \
			 ft_memset \
			 ft_strlcpy \
			 ft_strlcat \
			 ft_bzero \
			 ft_memcpy \
			 ft_strchr \
			 ft_strrchr \
			 ft_strnstr \
			 ft_memmove \
			 ft_calloc \
			 ft_memchr \
			 ft_memcmp \
			 ft_substr \
			 ft_strtrim \
			 ft_itoa \
			 ft_split \
			 ft_striteri \
			 ft_strmapi \

bfiles	=	ft_lstnew \
			ft_lstadd_front \
			ft_lstsize \
			ft_lstlast \
			ft_lstadd_back \
			ft_lstiter \
			ft_lstmap \
			ft_lstdelone \
			ft_lstclear \




Compiler	= gcc

CmpFlags	= -Wall -Wextra -Werror

AFILES	= $(Library).a

CFILES	= $(files:%=%.c)

OFILES	= $(files:%=%.o)

BCFILES = $(bfiles:%=%.c)

BOFILES = $(bfiles:%=%.o)

BLUE := \033[34m
RED := \033[0;31m
GREEN := \033[0;32m
PINK := \033[95m
CYAN := \033[0;36m
RESET := \033[0m


all: catall
	@$(Compiler) $(CmpFlags) -c $(CFILES) $(BCFILES) -I./
	@ar -rc $(AFILES) $(OFILES) $(BOFILES)

bonus:catbonus
	@$(Compiler) $(CmpFlags) -c $(BCFILES) -I./
	@ar -rc $(AFILES) $(BOFILES)

clean:catclean
	@rm -f $(OFILES)
	@rm -f $(BOFILES)

fclean: clean catfclean
	@rm -f $(AFILES)

re: fclean all catre

